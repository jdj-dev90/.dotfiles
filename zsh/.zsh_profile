alias ls='exa'
alias v='nvim'
alias tmux='tmux -u'
alias mysql="/usr/lib/mysql-workbench/mysql"
  
export DOTFILES=$HOME/.dotfiles
export SCRIPTS=$HOME/.dotfiles/bin/.local/scripts
export STOW_FOLDERS="bin,nvim,tmux,zsh,x11,screenlayout,wallpapers"
export EDITOR=/usr/bin/nvim
export PATH="/home/jordan/.scripts:/home/jordan/.local/bin:/home/jordan/.local/scripts:$PATH"
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion


export OPENSSL_DIR=/etc/ssl
export OPENSSL_LIB_DIR=/etc/ssl
. "$HOME/.cargo/env"
